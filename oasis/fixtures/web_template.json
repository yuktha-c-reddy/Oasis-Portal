  "name": "mission",
  "standard": 0,
  "template": "\n<style>\n    .about-mission{\n    padding: 64px 0px;\n    display: flex;\n    gap: 96px;\n}\n\n.about-mission-heading{\n    font-family: 'Bricolage Grotesque';\n    font-style: normal;\n    font-weight: 600;\n    font-size: 36px;\n    line-height: 124%;\n    letter-spacing: -0.04em;\n    color: #000000;\n}\n.about-mission-text{\n    font-family: 'Inter';\n    font-style: normal;\n    font-weight: 400;\n    font-size: 20px;\n    line-height: 136%;\n    letter-spacing: -0.01em;\n    color: #000000;\n    opacity: 0.7;\n}\n.mission-sub-container{\n    display: flex;\n    flex: 1;\n    gap: 60px;\n}\n.mission-box{\n    box-sizing: border-box;\n    display: flex;\n    flex-direction: column;\n    align-items: flex-start;\n    padding: 40px 0px;\n    gap: 20px;\n    border-top: 1px solid rgba(0, 0, 0, 0.16);\n}\n@media screen and (max-width: 768px) {\n    .about-main-container{\n        padding: 0px 16px;\n    }\n    .about-mission{\n        padding: 36px 0px;\n        flex-direction: column;\n        gap: 36px;\n    }\n    .about-mission-heading{\n        font-size: 24px;\n        line-height: 29px;\n    }\n    .mission-sub-container{\n        flex-direction: column;\n        gap: 0px;\n    }\n    .mission-box{\n        padding: 24px 0px 40px 0px;\n    }\n}\n</style>\n<div class=\"about-main-container\">\n<div class=\"about-mission fade-in\">\n            <div>\n                <p class=\"about-mission-heading\">Mission</p>\n            </div>\n            <div class=\"mission-sub-container\">\n                <div class=\"mission-box\">\n                    <p class=\"about-mission-text\">To address systemic technological gaps and lack of technological capacity in the social sector through the adoption of FOSS.</p>\n                </div>\n                <div class=\"mission-box\">\n                    <p class=\"about-mission-text\">To help organizations attain technological self-sufficiency to better focus on their social work and increase its impact.</p>\n                </div>\n                <div class=\"mission-box\">\n                    <p class=\"about-mission-text\">To help create a thriving ecosystem of FOSS innovation and collaboration in the social sector, made of diverse stakeholders.</p>\n                </div>\n            </div>\n        </div>\n</div>",
  "type": "Section"
 },
